{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "scbutler1-datafactory"
		},
		"ADLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS'"
		},
		"AzureSQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSQLDB'"
		},
		"reyesazuresynapse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'reyesazuresynapse'"
		},
		"ADLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reyestrainingdatalake.dfs.core.windows.net"
		},
		"MoviesGitHub_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/djpmsft/adf-ready-demo/master/moviesDB.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_accountKey": {
						"value": "[parameters('ADLS_accountKey')]"
					},
					"AzureSQLDB_connectionString": {
						"value": "[parameters('AzureSQLDB_connectionString')]"
					},
					"reyesazuresynapse_connectionString": {
						"value": "[parameters('reyesazuresynapse_connectionString')]"
					},
					"ADLS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_properties_typeProperties_url')]"
					},
					"MoviesGitHub_properties_typeProperties_url": {
						"value": "[parameters('MoviesGitHub_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}